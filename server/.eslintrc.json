{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true
  },
  "extends": "airbnb-base",
  "rules": {
      "indent": [
          "error",
          "tab"
      ],
      "linebreak-style": [
          "error",
          "windows"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "arrow-body-style": [
          "error",
          "always"
      ],
      "no-console": 0,
      "no-tabs": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0
  }
}